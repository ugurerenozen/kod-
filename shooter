package frc.robot.subsystems;


import com.revrobotics.spark.SparkMax;
import com.revrobotics.spark.SparkLowLevel.MotorType;

import edu.wpi.first.wpilibj2.command.SubsystemBase;

public class shooterSubsystems extends SubsystemBase {
  /** Creates a new shooterSubsystems. */
  SparkMax shooter_neo = new SparkMax(2, MotorType.kBrushless);
  SparkMax shooter_neo2 = new SparkMax(3, MotorType.kBrushless);
  public shooterSubsystems() {}

  @Override
  public void periodic() {
    // This method will be called once per scheduler run
  }
  public void shooter_run (double speed){
    shooter_neo.set(speed);
    shooter_neo2.set (speed);
  }
}
